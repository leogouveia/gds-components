{
  "name": "@leogouveia/gds-library",
  "version": "1.0.0",
  "description": "GDS Design System Components for React Native",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "react-native": "./src/index.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "react-native": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bob build",
    "watch": "bob build --watch",
    "clean": "rm -rf lib",
    "prepare-publish": "npm run build && node scripts/prepare-publish.js",
    "restore-dev": "node scripts/restore-dev.js",
    "publish-package": "npm run prepare-publish && npm publish && npm run restore-dev"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "~19.0.10",
    "react-native-builder-bob": "^0.40.13",
    "typescript": "~5.8.3"
  },
  "author": "Leonardo de Albuquerque Gouveia",
  "license": "ISC",
  "homepage": "https://github.com/leogouveia/gds-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leogouveia/gds-components.git"
  },
  "bugs": {
    "url": "https://github.com/leogouveia/gds-components/issues"
  },
  "keywords": [
    "react-native",
    "gds",
    "design-system",
    "components",
    "ui",
    "tamagui",
    "typescript",
    "ios",
    "android"
  ],
  "files": [
    "lib",
    "src",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tamagui/babel-plugin": "^1.132.15",
    "@tamagui/build": "^1.132.15",
    "@tamagui/config": "^1.132.15",
    "@tamagui/lucide-icons": "^1.132.15",
    "tamagui": "^1.132.15"
  }
}
