{
  "name": "@gds/components",
  "version": "1.0.0",
  "description": "Uma biblioteca de componentes de UI para React Native",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "react-native": "dist/jsx/index.js",
  "module:jsx": "dist/jsx/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "react-native": "./dist/jsx/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "author": "Leonardo de Albuquerque Gouveia",
  "license": "ISC",
  "files": [
    "types",
    "src",
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@tamagui/babel-plugin": "^1.132.15",
    "@tamagui/build": "^1.132.15",
    "@tamagui/config": "^1.132.15",
    "@tamagui/lucide-icons": "^1.132.15",
    "tamagui": "^1.132.15"
  }
}
